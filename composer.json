{
    "name": "burak-sevinc/pueue",
    "description": "Simple queue for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BurakSevinc\\Pueue\\": "src/",
            "BurakSevinc\\Pueue\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Burak Sevinc",
            "email": "info@buraksevinc.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "php-di/php-di": "^7.0",
        "aws/aws-sdk-php": "^3.304",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "doctrine/coding-standard": "^12.0",
        "squizlabs/php_codesniffer": "@stable"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
